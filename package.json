{
  "name": "YamaD.me",
  "version": "1.0.0",
  "dependencies": {
    "katex": "0.15.2",
    "sanitize.css": "13.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "autoprefixer": "10.4.2",
    "cssnano": "5.0.17",
    "mdsvex": "0.10.5",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.6",
    "postcss-load-config": "3.1.3",
    "prettier": "2.5.1",
    "rehype-katex": "5",
    "remark-math": "2",
    "sass": "1.49.8",
    "svelte": "3.46.4",
    "svelte-preprocess": "4.10.3",
    "svelte-sitemap": "2.3.0",
    "tslib": "2.3.1",
    "typescript": "4.5.5",
    "unist-util-visit": "2"
  },
  "scripts": {
    "build": "run-s build:kit build:404 build:sitemap build:pretty",
    "build:404": "mv build/404/index.html build/404.html && rmdir build/404",
    "build:kit": "svelte-kit build",
    "build:pretty": "prettier -w 'build/**/*.html'",
    "build:sitemap": "svelte-sitemap --trailing-slashes -d https://yamad.me -i '404.html'",
    "dev": "svelte-kit dev --host"
  },
  "type": "module"
}
