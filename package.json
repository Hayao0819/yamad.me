{
  "name": "YamaD.me",
  "version": "1.0.0",
  "dependencies": {
    "katex": "0.16.0",
    "sanitize.css": "13.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "autoprefixer": "10.4.7",
    "cssnano": "5.1.12",
    "js-beautify": "1.14.4",
    "mdsvex": "0.10.6",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.14",
    "postcss-load-config": "4.0.1",
    "rehype-katex-svelte": "1.1.2",
    "remark-math": "2",
    "sass": "1.53.0",
    "svelte": "3.49.0",
    "svelte-check": "2.8.0",
    "svelte-preprocess": "4.10.7",
    "svelte-sitemap": "2.5.1",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "vite": "3.0.0"
  },
  "scripts": {
    "_build": "run-s _build:kit _build:404 _build:sitemap _build:beautify",
    "_build:404": "mv build/404/index.html build/404.html && rmdir build/404",
    "_build:beautify": "html-beautify -r -s 2 --no-preserve-newlines 'build/**/*.html'",
    "_build:kit": "vite build",
    "_build:sitemap": "svelte-sitemap -d https://yamad.me -i '404.html'",
    "build": "run-s build-src _build",
    "build-font": "bash scripts/font.sh",
    "build-ipfs": "bash scripts/ipfs.sh",
    "build-src": "git archive --prefix=src/ -o static/src.tgz HEAD",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check-watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "dev-host": "vite dev --host",
    "prepare": "svelte-kit sync"
  },
  "type": "module"
}
