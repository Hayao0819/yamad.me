{
  "name": "YamaD.me",
  "version": "1.0.0",
  "dependencies": {
    "ayu": "8.0.1",
    "firacode": "6.2.0",
    "katex": "0.16.0",
    "sanitize.css": "13.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "autoprefixer": "10.4.7",
    "cssnano": "5.1.11",
    "mdsvex": "0.10.6",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.14",
    "postcss-load-config": "4.0.1",
    "prettier": "2.6.2",
    "rehype-katex-svelte": "1.1.2",
    "remark-math": "2",
    "sass": "1.52.3",
    "svelte": "3.48.0",
    "svelte-check": "2.7.2",
    "svelte-preprocess": "4.10.7",
    "svelte-sitemap": "2.5.1",
    "tslib": "2.4.0",
    "typescript": "4.7.3"
  },
  "scripts": {
    "build": "run-s build:kit build:404 build:sitemap build:pretty",
    "build:404": "mv build/404/index.html build/404.html && rmdir build/404",
    "build:kit": "svelte-kit build",
    "build:pretty": "prettier -w 'build/**/*.html'",
    "build:sitemap": "svelte-sitemap -d https://yamad.me -i '404.html'",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check-watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "svelte-kit dev",
    "dev-host": "svelte-kit dev --host"
  },
  "type": "module"
}
